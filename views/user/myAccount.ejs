    <script>
        async function cancelMyOrder(orderId, productId, quantity) {
            try {
                event.preventDefault()
                const response = await axios.post('/cancel-order', { orderId, productId, quantity });
                if (response.data.success) {
                    location.reload()
                }
            } catch (error) {
                console.error('Error cancelling order:', error);
            }
        }
    </script>
    <script>
        async function returnMyOrder(orderId, productId, quantity) {
            try {
                event.preventDefault()
                const reason = document.getElementById(`return-reason-${orderId}-${productId}`).value;
                const response = await axios.post('/return-order', { orderId, productId, quantity, reason });
                if (response.data.success) {
                    location.reload()
                }
            } catch (error) {
                console.error('Error returning order:', error);
            }
        }
    </script>
    <script>
function payNow(orderId) {
    // Make an AJAX request to update the order status
    fetch('/updateOrderStatus', {
        method: 'POST',
        headers: {
            'Content-Type': 'application/json'
        },
        body: JSON.stringify({ orderId: orderId, status: 'retry done' })
    })
    .then(response => response.json())
    .then(data => {
        if (data.success) {
            // Redirect to the checkout page after updating the status
            window.location.href = '/checkout'
        } else {
            console.error('Failed to update order status:', data.message);
        }
    })
    .catch(error => console.error('Error:', error));
}
</script>
