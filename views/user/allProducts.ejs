<!-- all products  -->
<div class="tab-content wow fadeIn animated" id="myTabContent">
    <div class="tab-pane fade show active" id="tab-one" role="tabpanel" aria-labelledby="tab-one">
        <div class="row product-grid-4">
            <% products.forEach(product => { %>
                <div class="col-lg-4 col-md-4 col-12 col-sm-6">
                    <div class="product-cart-wrap mb-30">
                        <div class="product-img-action-wrap">
                            <div class="product-img product-img-zoom">
                                <a href="/product/<%= product._id %>">
                                    <!-- Load primary image -->
                                    <img class="default-img" src= "/admin-assets/imgs/items/<%= product.primary_image.name %>" alt="<%= product.product_name %>" width="10px" height="280px">
                                    <!-- Adjust image path and alt text accordingly -->
                                </a>
                            </div>
                         
                            <!-- Add badges or labels here if needed -->
                        </div>
                        <div class="product-content-wrap">
                            <div class="product-category">
                                <a href="#"><%= product.category_id.cat_name %></a>
                                <!-- Assuming category name is available in product data -->
                            </div>
    
                            <h2><a href="/product/<%= product._id %>"><%= product.product_name %></a></h2>
                            <!-- You can add more product details here -->
                            <% if(product.selling_price != product.actual_price){ %>
                                <div class="rating-result" title="offer">

                                    <span>
                                    
                                        <span><b><%= product.category_id.offer %>%</b></span>
                                    </span>
                                </div>
                                <% } %>
                                <% if(product.selling_price != product.actual_price){ %>
                                    <div class="product-price">
                                        <span>$<%= product.selling_price %></span>
                            
                                        <span class="old-price">$<%= product.actual_price %></span>
                                    
                                    </div>
                                    <% } %>
                                   <%  if(product.selling_price == product.actual_price){ %>
                                    <div class="product-price">
                                    <span>$<%= product.actual_price %></span>
                                </div>
                                    <% } %>
                           
                        </div>
                    </div>
                </div>
            <% }); %>
        </div>
    </div>
