<script>
    async function updateQuantity(productId, operator) {
    try {
        event.preventDefault();
        const response = await axios.post('/update-quantity', { productId, operator });
        
        if (response.data.success) {
            // Update total quantities and subtotal on the page
            document.getElementById('totalQuantities').innerText = response.data.totalQuantities;
            document.getElementById('subtotal').innerText = '$' + response.data.subtotal.toFixed(2);
            
            // Update quantity field in the cart table
            document.getElementById(`quantity-${productId}`).value = response.data.productQuantity;
    
            // Update subtotal for the specific product
            document.getElementById(`subtotal-value-${productId}`).innerText =response.data.productPrice;
        }else{
            alert(response.data.message)
        }
    } catch (error) {
        console.error('Error updating quantity:', error);
    }
}

</script>
    
    
    
    
    
    
    
